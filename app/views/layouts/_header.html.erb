 <header class="navbar navbar-fixed-top navbar-expand-lg navbar-light bg-light">
   <div class="container">
     <%= link_to "Bake up", root_path, id: "logo", class: "fs-2 navbar-brand text-dark" %>
     <nav>
       <ul class="nav navbar-nav navbar-right">
         <li><%= link_to "Home", root_path %></li>
         <% if logged_in? %>
         <li><%= link_to 'ユーザー一覧', users_path %></li>
         <li class="dropdown">
           <a href="#" class="dropdown-toggle" data-toggle="dropdown">
             アカウント <b class="caret"></b>
           </a>
           <ul class="dropdown-menu">
             <li><%= link_to 'プロフィール', current_user %></li>
             <li><%= link_to '編集', "#" %></li>
             <li class="divider"></li>
             <li><%= link_to 'ログアウト', logout_path, method: :delete %></li>
           </ul>
         </li>
         <% else %>
         <li><%= link_to "ユーザー登録", signup_path %></li>
         <li><%= link_to "ログイン", login_path %></li>
         <% end %>
       </ul>
     </nav>
   </div>
 </header>